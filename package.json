{
  "name": "casarnos.com.br",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm run services:up && next dev --turbopack",
    "services:up": "docker compose -f src/app/infra/docker-compose.yaml up -d",
    "services:down": "docker compose -f src/app/infra/docker-compose.yaml down",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "migrate": "node-pg-migrate",
    "migrate:create": "node-pg-migrate create -m migrations",
    "migrate:up": "node-pg-migrate up"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-slot": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "config": "^4.1.0",
    "framer-motion": "^12.4.10",
    "lucide-react": "^0.477.0",
    "next": "15.2.0",
    "node-pg-migrate": "^8.0.3",
    "pg": "^8.14.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/pg": "^8.11.13",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.20",
    "eslint": "^9",
    "eslint-config-next": "15.2.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.0.9",
    "typescript": "^5"
  }
}
